{
  "private": true,
  "name": "play-sdk-example",
  "version": "0.0.1",
  "type": "module",
  "description": "Example usage of PlayHT SDK",
  "scripts": {
    "build": "rm -rf ./dist && tsc -b",
    "build:client": "cd ./client && yarn build",
    "start": "yarn build:client && yarn build && nodemon dist/server.js",
    "lint": "prettier --write . && eslint --ext .ts ./src --fix && tsc --noEmit"
  },
  "dependencies": {
    "@playht/playht": "0.0.4",
    "@types/multer": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "nodemon": "^3.0.1",
    "prettier": "3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "@playht/playht": "portal:../playht"
  }
}
